{
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"plugins": ["react"],
	"parserOptions": {
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"jasmine": true
	},
	"rules": {
		"no-extra-semi": 1,
		"no-unused-vars": [1, {
			"args": "none"
		}],
		"no-console" : [1, { "allow": ["warn", "error", "info"]}],
		"indent": [1, "tab"],
		"curly": [1, "all"],
		"semi": [1, "always"],
		"dot-notation": 1,
		"react/prop-types": 0,
		"react/display-name": 0,
		"react/jsx-filename-extension": [1, { "extensions": [".jsx"] }]
	}
}
